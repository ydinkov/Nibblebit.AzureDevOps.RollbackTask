{
    "manifestVersion": 1,
    "id": "Rollback",
    "name": "Rollback Deployment",
    "version": "0.3.11",
    "publisher": "Nibblebit",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "This task finds the precious successful release to of the same stage and redeploys jobs in that stage",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/logo.png"        
    },
    "files": [
        {
            "path": "."
        }
    ],
    "contributions": [
        {
            "id": "rollback-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "rollback-task"
            }
        }
    ]
}
